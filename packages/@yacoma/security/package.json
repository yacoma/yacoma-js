{
  "name": "@yacoma/security",
  "version": "0.0.0",
  "description": "Scurity rules for data and server.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yacoma/yacoma-js.git"
  },
  "author": "Gaspard Bucher <gaspard@lucidogen.io>",
  "contributors": ["Henri Hulski <henri@yacoma.it>"],
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p tsconfig/${NODE_ENV:=development}.json --outDir ../../node_modules/@yacoma/security/dist",
    "clean": "rimraf .cache dist coverage ../../node_modules/@yacoma/@yacoma/security",
    "postbuild": "cp package.json ../../node_modules/@yacoma/security",
    "posttest": "npm run typecheck",
    "prebuild": "npm run clean",
    "prepare": "rimraf dist && cp -r ../../node_modules/@lucidogen/security/dist dist",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --updateSnapshot --coverage false",
    "typecheck": "tsc -p tsconfig/${NODE_ENV:=development}.json --noEmit"
  },
  "dependencies": {
    "@yacoma/build": "next",
    "@yacoma/crypt": "next"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/simple-mock": "^0.8.1",
    "jest": "^24.8.0",
    "simple-mock": "^0.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.3.4000"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "import-sort-style-module",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "import-sort-style-module",
      "options": {}
    }
  }
}
