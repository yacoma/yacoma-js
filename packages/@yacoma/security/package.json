{
  "name": "@lucidogen/security",
  "version": "0.0.0",
  "description": "Scurity rules for data and server.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucidogen/lucidogen.git"
  },
  "author": "Gaspard Bucher <gaspard@lucidogen.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p setup/tsconfig.${NODE_ENV:=development}.json",
    "build:watch": "tsc -w -p setup/tsconfig.${NODE_ENV:=development}.json",
    "postbuild": "cp package.json ../../node_modules/@lucidogen/security",
    "prepare": "rimraf dist && cp -r ../../node_modules/@lucidogen/security/dist dist",
    "clean": "rimraf .cache dist coverage ../../node_modules/@lucidogen/security",
    "test": "jest -c setup/jest.js",
    "test:watch": "jest -c setup/jest.js --watch --coverage false"
  },
  "dependencies": {
    "@lucidogen/build": "next",
    "@lucidogen/crypt": "next"
  },
  "devDependencies": {
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "typescript": "^3.3.1"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "import-sort-style-module",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "import-sort-style-module",
      "options": {}
    }
  }
}
