{
  "name": "@lucidogen/hooks",
  "version": "0.0.0",
  "description": "Support for callable hooks in actions (through effects).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucidogen/lucidogen.git"
  },
  "author": "Gaspard Bucher <gaspard@lucidogen.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p setup/tsconfig.${NODE_ENV:=development}.json",
    "postbuild": "cp package.json ../../node_modules/@lucidogen/hooks",
    "prepare": "rimraf dist && cp -r ../../node_modules/@lucidogen/hooks/dist dist",
    "clean": "rimraf .cache dist coverage ../../node_modules/@lucidogen/hooks",
    "test": "jest -c setup/jest.js",
    "test:watch": "jest -c setup/jest.js --watch --coverage false"
  },
  "dependencies": {
    "@lucidogen/build": "next",
    "overmind": "^5.1.2"
  },
  "devDependencies": {
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "typescript": "^3.3.1"
  }
}
