{
  "name": "@yacoma/build",
  "version": "0.0.0",
  "description": "Help create modular apps with blocks.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yacoma/yacoma-js.git"
  },
  "author": "Gaspard Bucher <gaspard@lucidogen.io>",
  "contributors": ["Henri Hulski <henri@yacoma.it>"],
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p tsconfig/${NODE_ENV:=development}.json --outDir ../../node_modules/@yacoma/build/dist",
    "clean": "rimraf .cache dist coverage ../../node_modules/@yacoma/@yacoma/build",
    "postbuild": "cp package.json ../../node_modules/@yacoma/build",
    "posttest": "npm run typecheck",
    "prebuild": "npm run clean",
    "prepare": "rimraf dist && cp -r ../../node_modules/@lucidogen/build/dist dist",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --updateSnapshot --coverage false",
    "typecheck": "tsc -p tsconfig/${NODE_ENV:=development}.json --noEmit"
  },
  "dependencies": {
    "overmind": "next"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.3.4000"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "import-sort-style-module",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "import-sort-style-module",
      "options": {}
    }
  }
}
