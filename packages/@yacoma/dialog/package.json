{
  "name": "@lucidogen/dialog",
  "version": "0.0.0",
  "description": "Modal dialogs management.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucidogen/lucidogen.git"
  },
  "author": "Gaspard Bucher <gaspard@lucidogen.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p setup/tsconfig.${NODE_ENV:=development}.json",
    "clean": "rimraf .cache dist coverage ../../node_modules/@lucidogen/dialog",
    "postbuild": "cp package.json ../../node_modules/@lucidogen/dialog",
    "prepare": "rimraf dist && cp -r ../../node_modules/@lucidogen/dialog/dist dist",
    "stories": "parcel serve --no-autoinstall src/stories/index.html",
    "test:watch": "jest -c setup/jest.js --watch --coverage false",
    "test": "jest -c setup/jest.js"
  },
  "dependencies": {
    "@lucidogen/build": "next",
    "classnames": "^2.2.6",
    "overmind-react": "next",
    "overmind": "next",
    "react-dom": "^16.8.1",
    "react": "^16.8.1",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@lucidogen/story": "next",
    "@types/styled-components": "^4.0.1",
    "jest": "^23.5.0",
    "parcel-bundler": "^1.11.0",
    "ts-jest": "^23.1.3",
    "typescript": "^3.3.1"
  }
}
