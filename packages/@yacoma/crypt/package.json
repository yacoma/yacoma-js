{
  "name": "@lucidogen/crypt",
  "version": "0.0.0",
  "description": "Encryption and signing (stored as JWT).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucidogen/lucidogen.git"
  },
  "author": "Gaspard Bucher <gaspard@lucidogen.io>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p setup/tsconfig.${NODE_ENV:=development}.json",
    "build:watch": "tsc -w -p setup/tsconfig.${NODE_ENV:=development}.json",
    "postbuild": "cp package.json ../../node_modules/@lucidogen/crypt",
    "prepare": "rimraf dist && cp -r ../../node_modules/@lucidogen/crypt/dist dist",
    "clean": "rimraf .cache dist coverage ../../node_modules/@lucidogen/crypt",
    "test": "jest -c setup/jest.js",
    "test:watch": "jest -c setup/jest.js --watch --coverage false"
  },
  "dependencies": {
    "base64url": "3.0.0",
    "buffer": "^5.2.1",
    "pako": "^1.0.8",
    "tweetnacl-ts": "^1.0.2"
  },
  "devDependencies": {
    "jest": "^23.5.0",
    "node-webcrypto-ossl": "^1.0.38",
    "simple-mock": "0.8.0",
    "text-encoder": "^0.0.4",
    "ts-jest": "^23.1.3",
    "typescript": "^3.3.1"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "import-sort-style-module",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "import-sort-style-module",
      "options": {}
    }
  }
}
